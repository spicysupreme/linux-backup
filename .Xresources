URxvt.scrollBar: false
URxvt.font: xft:kakwafont:size=8
URxvt.boldFont: xft:kakwafont:bold:size=8
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.internalBorder: 15
!! COLORS
! special
*.foreground:   #bea492
*.background:   #181512
*.cursorColor:  #bea492

! black
*.color0:       #282522
*.color8:       #484542

! red
*.color1:       #8c4c4c
*.color9:       #ae5d5d

! green
*.color2:       #746c48
*.color10:      #8a8660

! yellow
*.color3:       #907245
*.color11:      #aa915b

! blue
*.color4:       #4c626d
*.color12:      #6a888f

! magenta
*.color5:       #604c5f
*.color13:      #7d6a7b

! cyan
*.color6:       #4b5e57
*.color14:      #607f77

! white
*.color7:       #a69181
*.color15:      #bea492

#define FOREGROUND #bea492
#define BACKGROUND #181512

#define COLOR0  #282522
#define COLOR8  #484542
#define COLOR1  #8c4c4c
#define COLOR9  #ae5d5d
#define COLOR2  #746c48
#define COLOR10 #8a8660
#define COLOR3  #907245
#define COLOR11 #aa915b
#define COLOR4  #4c626d
#define COLOR12 #6a888f
#define COLOR5  #604c5f
#define COLOR13 #7d6a7b
#define COLOR6  #4d5e57
#define COLOR14 #607f77
#define COLOR7  #a69181
#define COLOR15 #bea492




rofi.font: kakwafont 8
rofi.width: 500
! rofi.theme: /usr/share/rofi/themes/Monokai
rofi.drun-display-format: {name}
rofi.color-window: BACKGROUND, BACKGROUND, BACKGROUND
rofi.color-normal: BACKGROUND, FOREGROUND, BACKGROUND, BACKGROUND, COLOR1
rofi.color-active: BACKGROUND, FOREGROUND, BACKGROUND, BACKGROUND, COLOR1
rofi.color-urgent: BACKGROUND, FOREGROUND, BACKGROUND, BACKGROUND, COLOR1
rofi.hide-scrollbar: true
rofi.padding: 20
rofi.separator-style: solid
rofi.location: 0
rofi.lines: 10
rofi.columns:1
rofi.bw: 10
